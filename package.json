{
  "name": "rest-express",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev:server": "npm --prefix server run dev",
    "dev:client": "npm --prefix client run dev",
    "dev": "node scripts/preflight.js && npx concurrently -n \"SERVER,CLIENT\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run dev:server\" \"npm run dev:client\"",
    "build:client": "npm --prefix client run build",
    "build:server": "npm --prefix server run build",
    "build": "npm run build:client && npm run build:server",
    "start": "npm --prefix server run start",
    "check": "concurrently --success=all -n CLIENT,SERVER -c cyan,magenta \"npm --prefix client run check\" \"npm --prefix server run check\"",
    "db:push": "drizzle-kit push",
    "test": "mocha tests/**/*.test.js --timeout 10000",
    "test:uid": "mocha tests/unified-uid.test.js --timeout 5000",
    "audit:comments": "node scripts/find-commented-blocks.mjs",
    "audit:commented-code": "node scripts/find-commented-code.mjs",
    "prebuild": "npm run check"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/jest": "^30.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "drizzle-kit": "^0.30.4",
    "eslint": "^9.32.0",
    "mocha": "^10.8.2",
    "prettier": "^3.6.2",
    "supertest": "^6.3.4",
    "tsx": "^4.20.5",
    "typescript": "^5.6.3",
    "vite": "^5.3.5"
  },
  "overrides": {
    "drizzle-orm": "$drizzle-orm",
    "drizzle-zod": "$drizzle-zod"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.1",
    "@supabase/supabase-js": "^2.57.4",
    "@types/date-fns": "^2.5.3",
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0.0",
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.2",
    "drizzle-orm": "^0.44.5",
    "drizzle-zod": "^0.8.3",
    "express": "^5.1.0",
    "jose": "^6.1.0",
    "multer": "^2.0.2",
    "nanoid": "^5.1.5",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.1.5"
  }
}
